<%- include('common/dashboard-header') %>
	<span class="dashboard-title">Actions</span>
	<span class="dashboard-subtitle">Summary of Token actions</span>
	<span class="dashboard-description">
		Token action is a action that related in token trensfer.
	</span>

	<div class="token">
		<div class="token-section">
			<span class="token-section-information">
				Search
				<span style="color: #ff5252;">(send token action)</span>
			</span>
			<div class="token-section-summary">
				<div class="token-section-summary-left">
					<div class="token-section-summary-user">
						<div class="token-section-summary-user-picture header-profile-picture"></div>
						<div class="token-section-summary-user-name header-profile-name">&nbsp;</div>
					</div>
					<div class="token-section-summary-arrow"></div>
					<div class="token-section-summary-user">
						<div class="token-section-summary-user-picture" style="background-image: url(/img/icon_logo_profile.png);"></div>
						<div class="token-section-summary-user-name">mycat</div>
					</div>
				</div>
				<div class="token-section-summary-right">
					<div class="token-section-summary-amount">
						<div class="token-section-summary-amount-label"># of actions</div>
						<div class="token-section-summary-amount-number">12</div>
						<div class="clearfix"></div>
					</div>
					<div class="token-section-summary-amount">
						<div class="token-section-summary-amount-label">Send</div>
						<div class="token-section-summary-amount-number">12.0000</div>
						<div class="clearfix"></div>
					</div>
				</div>
				<div class="clearfix"></div>
			</div>
		</div>
		<div class="token-section">
			<span class="token-section-information">
				Distribution
				<span style="color: #448aff;">(receive token action)</span>
			</span>
			<div class="token-section-summary">
				<div class="token-section-summary-left">
					<div class="token-section-summary-user">
						<div class="token-section-summary-user-picture" style="background-image: url(/img/icon_logo_profile.png);"></div>
						<div class="token-section-summary-user-name">mycat</div>
					</div>
					<div class="token-section-summary-arrow"></div>
					<div class="token-section-summary-user">
						<div class="token-section-summary-user-picture header-profile-picture"></div>
						<div class="token-section-summary-user-name header-profile-name">&nbsp;</div>
					</div>
				</div>
				<div class="token-section-summary-right">
					<div class="token-section-summary-amount">
						<div class="token-section-summary-amount-label"># of actions</div>
						<div class="token-section-summary-amount-number">26</div>
						<div class="clearfix"></div>
					</div>
					<div class="token-section-summary-amount">
						<div class="token-section-summary-amount-label">Send</div>
						<div class="token-section-summary-amount-number">26.0000</div>
						<div class="clearfix"></div>
					</div>
				</div>
				<div class="clearfix"></div>
			</div>
		</div>
		<div class="clearfix"></div>
	</div>


	<span class="dashboard-subtitle">Point action</span>
	<span class="dashboard-description">
		Point action is a action that related in point accumulate.
	</span>
	<div class="point-summary">
		<table cellspacing="0">
			<tr>
				<th width="50%">Actions</th>
				<th width="50%"># of actions</th>
			</tr>
		</table>
		<table cellspacing="0">
			<tr>
				<td width="50%">confirm</td>
				<td width="50%">32</td>
			</tr>
			<tr>
				<td width="50%">tag</td>
				<td width="50%">4</td>
			</tr>
			<tr>
				<td width="50%">original</td>
				<td width="50%">32</td>
			</tr>
		</table>
	</div>
	<div class="actions-log-list">
		<div class="actions-log-list-item">
			<span class="actions-log-list-item-type" style="color: ${action_type_color}">${action_type}</span>
			<span class="actions-log-list-item-description">${text}</span>
		</div>
	</div>
	<div class="clearfix"></div>

	<script src="https://cdn.rawgit.com/honeymaro/MaroJS-list-renderer/673631d6/src/list-renderer.js"></script>
	<script src="/js/common.js"></script>
	<script src="/js/dashboard/common.js"></script>
	<script src="/js/dashboard/actions.js"></script>
	<%- include('common/dashboard-footer') %>